<% include ../partials/header %>



<div class="container">
<header class="jumbotron">
    <div class="container">
       <h1>דוקטור חומוס</h1>
       <p>צפה בביקורות מקצועיות על חומוסיות</p>
       <p>
        <a class="btn btn-primary btn-lg" href="/createrest/new"> הוסף ביקורת חדשה </a>
       </p>
       </div>
</header>






<div class="row text-center" style="display:flex; flex-wrap: wrap;">
  <% hummus.forEach(function(hummuss){ %>
    <div class="col-md-3 col-sm-6">
        <div class="thumbnail">
            <img style="height: 150px;" src="<%= hummuss.image %>">
            <div class="caption">
                 <h4><%= hummuss.name %><br> ציון:
                 <%= hummuss.rating %>/10</h4>
            </div>
            <p>
                <a href="/restaurant/<%= hummuss._id %>" class="btn btn-primary">לצפיה בביקורת</a>
                
            </p>
        </div>
  
    
    </div>
    <% }); %>
    </div>
</div>

<% include ../partials/footer %>